<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/comm_background"
        android:orientation="vertical">



        <include layout="@layout/item_quan_top"/>

       <cn.wayne.mamypoko.ui.NoScrollListView
            android:layout_marginTop="10dp"
            android:id="@+id/list_top"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:divider="@color/comm_line_color"
            android:listSelector="@drawable/common_btn_bg"
            android:dividerHeight="0.67dp"
            android:cacheColorHint="@color/translate"
            />

        <cn.wayne.mamypoko.ui.NoScrollListView
            android:layout_marginTop="10dp"
            android:id="@android:id/list"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:divider="@color/translate"
            android:listSelector="@drawable/common_btn_bg"
            android:dividerHeight="10dp"
            android:cacheColorHint="@color/translate"
            />

</LinearLayout>
